{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "rootDir": "./wwwroot",
    "sourceMap": true,
    "target": "es5",
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "node_modules"//,
    //"wwwroot"
  ]
}
